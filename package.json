{
  "name": "test-ui-library-2",
  "version": "0.1.1",
  "author": "Kirill Evtushenko",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/infalse/test-ui-package-2#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/infalse/test-ui-package-2.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "NODE_ENV=production tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/test-ui-library-2.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test-ui-library-2.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-is": "^18.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^7.0.8",
    "tailwindcss": "^3.0.24",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  }
}
